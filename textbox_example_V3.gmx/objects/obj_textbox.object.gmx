arrow_drawer=0
chr_=0
skipchr=0
speed_mode=3
sound_type=snd_voicemid
str_edited=""
shaking=0
view_xstart=view_xview[view_current]
view_ystart=view_yview[view_current]
command=0
sliding=1
drawbox_y=62
drawbox2_y=-50
</string>
          </argument>
        </arguments>
      </action>
