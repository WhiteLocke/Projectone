        <arguments>
          <argument>
            <kind>1</kind>
            <string>//screen shake effect
if shaking=1
{
  view_xview[view_current]=view_xstart+choose(-2,-1,0,1,2)
  view_yview[view_current]=view_ystart+choose(-2,-1,0,1,2)
}
else
{
  view_xview[view_current]=view_xstart
  view_yview[view_current]=view_ystart
}
//sliding
if sliding=1
{
  drawbox2_y+=1
  if drawbox_y&gt;38
  {
    drawbox_y-=3
  }
  else
  {
    if drawbox_y&gt;6
    {
      drawbox_y-=2
    }
    else
    {
      drawbox_y-=1
      if drawbox_y=0
      {
        sliding=0
        alarm[1]=3
        scr_stredit()
      }
    }
  }
}
if sliding=-1
{
  drawbox_y+=8
  drawbox2_y-=6
  if drawbox_y&gt;=64
  {
    instance_destroy()
  }
}
</string>
          </argument>
        </arguments>
      </action>
