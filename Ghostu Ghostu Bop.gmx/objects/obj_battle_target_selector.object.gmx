          <argument>
            <kind>1</kind>
            <string>if(defaultSet){        
 
       /* Draw the arrow and the help text */
    if(allySelected &amp;! groupSelected)
    {
            arrowx = Player.party[targetSelected].x
            arrowy = Player.party[targetSelected].y
            obj_battle_help_box.text = Player.party[targetSelected].name
    }         

    if(enemySelected &amp;! groupSelected)
    {
            arrowx = obj_battle_controller.enemies[targetSelected].x
            arrowy = obj_battle_controller.enemies[targetSelected].y
            obj_battle_help_box.text = obj_battle_controller.enemies[targetSelected].name
    }
    
    
    /* END ARROW DRAWING */

    if (keyboard_check_pressed(vk_down)){
       /* We are selecting allies */     
            if(allySelected){
                if (targetSelected = Player.numPartyMembers-1)
                    targetSelected = 0
                else
                    targetSelected += 1

            }      
            
            /* We are selecting enemies */     
            if(enemySelected){
                if (targetSelected = obj_battle_controller.numEnemies-1)
                    targetSelected = 0
                else
                    targetSelected += 1

            }      
    }
        if (keyboard_check_pressed(vk_up)){
            if(allySelected){
                if (targetSelected = 0)
                    targetSelected = Player.numPartyMembers-1
                else
                    targetSelected -= 1
            }
            else if(enemySelected){
                if (targetSelected = 0)
                    targetSelected = obj_battle_controller.numEnemies-1
                else
                    targetSelected -= 1
            
            }
        }
       if(keyboard_check_pressed(vk_enter)){
            if(groupSelected = false){
                if(allySelected)
                   script_execute(script,Player.party[targetSelected])
                if(enemySelected)
                   script_execute(script,obj_battle_controller.enemies[targetSelected])
            }
        }
    
    if(keyboard_check_pressed(vk_escape))
    { 
        //return to the parent menu
    
                //Give my parent focus
                audio_play_sound(snd_back,10,0)
                parentID.inFocus = true
                
                //Kill myself
                instance_destroy()
            
    }
    
}
</string>
